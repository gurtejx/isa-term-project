{
    "version": 2,
    "env": {
      "NODE_SESSION_SECRET": "@node_session_secret",
      "MONGODB_URI": "@mongodb_uri",
      "JWT_SECRET": "@jwt_secret",
      "EMAIL_SERVICE_PROVIDER": "@email_service_provider",
      "APP_EMAIL": "@app_email",
      "APP_EMAIL_PASSWORD": "@app_email_password"
    },
    "builds": [
      {
        "src": "server.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ]
  }